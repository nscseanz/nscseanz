{
  "type": "object",
  "comment": "IssuseList",
  "properties": {
    "IssuseList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ID": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 3
          },
          "Status": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 9,
            "control": "property.innerText"
          },
          "Summary": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 10,
            "control": "property.innerText"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": "#buglist tbody tr[bgcolor]"
    },
    "IsViewIssues": {
      "type": "identifier",
      "selector": ".floatleft",
      "control": "exists",
      "selectContains": "Viewing issues"
    },
    "reportissuselink": {
      "type": "action",
      "selector": "a[href='/mantisbt/bug_report_page.php']",
      "control": "action.link"
    }
  },
  "connect": [
    {
      "match": [
        "IsViewIssues"
      ],
      "screens": [
        "issuselist"
      ]
    },
    {
      "match": [],
      "action": "reportissuselink"
    }
  ]
}
